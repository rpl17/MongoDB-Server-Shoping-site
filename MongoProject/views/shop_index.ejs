<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Ryan's Shop</title>
    <link rel="stylesheet" type="text/css" href="/css/style.css">
    <link href='https://fonts.googleapis.com/css?family=Open+Sans:300' rel='stylesheet' type='text/css'>
  </head>
  <body>
    <div class="container">
      <fieldset>
        <form action="/show_catalog" method="post">
          <input type="submit" class="ghost-button" value="Show Shop Catalog">
        </form>
      </fieldset>
    </div>
    <div class="container">
      <fieldset>
        <form action="/show_cart" method="post">
          <input type="submit" class="ghost-button" value="View Cart">
        </form>
      </fieldset>
    </div>
	<div class="container">
      <fieldset>
        <form action="/past_orders" method="post">
          <input type="submit" class="ghost-button" value="Past Orders">
        </form>
      </fieldset>
    </div>
    <div class="container">
      <fieldset>
        <form action="/generate_fakedata" method="post">
          <input type="submit" class="ghost-button" value="Generate Fake Data">
        </form>
      </fieldset>
    </div>
	<div class="container">
      <fieldset>
        <form action="/login_home" method="post">
          <input type="submit" class="ghost-button" value="Logout">
        </form>
      </fieldset>
    </div>
  </body>
</html>
